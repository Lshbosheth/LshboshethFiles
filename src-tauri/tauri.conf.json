{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run start",
    "devPath": "http://localhost:1030",
    "distDir": "../dist"
  },
  "package": {
    "productName": "File Manage",
    "version": "0.0.4"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "cn.lshbosheth.file",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://lshbosheth.github.io/LshboshethFiles/version.json",
        "http://oss.lshbosheth.cn/version.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDRBQjJBQUVGMTVDNUVGNTkKUldSWjc4VVY3NnF5U2dvY003TCtPT0FpYXhEaThxTEc2MkFiaWZyZGV2WmRaNGYwRFpoRDZFcmoK",
      "windows": {
        "installMode": "basicUi"
      }
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 800,
        "resizable": false,
        "title": "",
        "skipTaskbar": false,
        "x": 0,
        "y": 0,
        "center": true,
        "decorations": false,
        "width": 1000
      }
    ]
  }
}